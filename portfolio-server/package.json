{
  "name": "portfolio-server",
  "version": "1.0.0",
  "description": "Backend API server for FoodHub portfolio application with Razorpay payment integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "echo \"No build step required for Node.js server\"",
    "lint": "echo \"Linting not configured yet\"",
    "seed": "node scripts/seed.js",
    "db:setup": "node scripts/setup-db.js",
    "db:test": "node scripts/test-mssql-connection.js",
    "db:diagnose": "node scripts/diagnose-mssql.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
